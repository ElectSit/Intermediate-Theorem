def f(x):

    return x ** 3-5*x-9

a=float(input('Enter value of a:'))

b=float(input('Enter value of b:'))

i=1

while(i<10):

    c=(a+b)/2
    print('Itration=%d,a=%f,b=%f,c=%f,f(a)=%f,f(b)=%f,f(c)=%f'%(i,a,b,c,f(a),f(b),f(c)))
    if f(a)*f(c)<0:

        b=c

    else:

        a=c

    i=i+1

print('\n Requird root is:%f'%c)

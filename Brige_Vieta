def f(x):

    return x*4-3*x*3+3*x*2-3*x+2
    
p0=float(input("enter value of po"))
a0=float(input("Enter value of a0"))
a1=float(input("enter value of a1"))
a2=float(input("enter value of a2"))
a3=float(input("enter value of a3"))
a4=float(input("enter value of a4"))
i=int(input("enter number of itration"))

count=0

while(count <= 1):

    b0=a0

    b1=a1+p0*b0

    b2=a2+p0*b1

    b3=a3+p0*b2

    b4=a4+p0*b3

    c0=b0

    c1=b1+p0*c0

    c2=b2+p0*c1

    c3=b3+p0*c2

    p1=p0-(b4/c3)

    print("iteration,count")

    p0=p1

    count=count+1

    print("root of given equations:",p1)
